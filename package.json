{
  "name": "@massimo-cassandro/js-utilities",
  "description": "My collection of js utilities",
  "version": "1.19.1",
  "license": "MIT",
  "author": "Massimo Cassandro",
  "main": "index.js",
  "scripts": {
    "UPD-version": "npx update-version --default-descr=fix",
    "npm-publish": "npm publish",
    "upd@m": "npx upd@m",
    "local test server": "python3 -m http.server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "files": [
    "src/**/*.{scss,css,js,map}",
    "scss/**/*.{scss,css,js,map}",
    "react-hooks/**/*.{scss,css,js,jsx,map}",
    "index.js"
  ],
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@massimo-cassandro/dev-updater": "^2.0.0",
    "@massimo-cassandro/eslint-config": "^2.0.6",
    "@massimo-cassandro/stylelint-config": "^1.1.9",
    "@stylistic/stylelint-plugin": "^3.1.2",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "stylelint": "^16.17.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-twbs-bootstrap": "^16.0.0"
  }
}